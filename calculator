def calculator(a, b, operator):
    if operator == "+":
        return a + b
    elif operator == "-":
        return a - b
    elif operator == "*":
        return a * b
    elif operator == "/":
        return a / b if b != 0 else "除零错误"
    else:
        return "无效的运算符"

print(calculator(10, 5, "*"))
